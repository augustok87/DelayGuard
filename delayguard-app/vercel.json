{
  "version": 2,
  "name": "delayguard",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/health",
      "dest": "/api/index.ts"
    },
    {
      "src": "/webhooks",
      "dest": "/api/index.ts"
    },
    {
      "src": "/auth",
      "dest": "/api/index.ts"
    },
    {
      "src": "/monitoring",
      "dest": "/api/index.ts"
    },
    {
      "src": "/$",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "framework": null
}
