{
  "version": 2,
  "name": "delayguard",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index.ts"
    },
    {
      "source": "/health",
      "destination": "/api/index.ts"
    },
    {
      "source": "/webhooks",
      "destination": "/api/index.ts"
    },
    {
      "source": "/auth",
      "destination": "/api/index.ts"
    },
    {
      "source": "/monitoring",
      "destination": "/api/index.ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "framework": null
}
